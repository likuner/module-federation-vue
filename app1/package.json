{
  "name": "app1",
  "version": "1.0.0",
  "description": "a template project based on vue3 & webpack5",
  "scripts": {
    "start": "npm run serve",
    "serve": "cross-env NODE_ENV=development BASE_PATH=/ webpack serve",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress",
    "build:zip": "cross-env BUILD_ZIP=true npm run build",
    "build:check": "cross-env CHECK_MODE=true npm run build",
    "dev": "cross-env NODE_ENV=development webpack --progress",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress",
    "build:pwa": "cross-env NODE_ENV=production PWA_MODE=true webpack --config ./webpack.pwa.config.js --progress",
    "dev:pwa": "cross-env NODE_ENV=development PWA_MODE=true webpack --config ./webpack.pwa.config.js --progress",
    "http-server": "http-server dist",
    "fix": "eslint ./ --ext .js,.vue --fix",
    "lint": "eslint ./ --ext .js,.vue",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "vue3",
    "template",
    "webpack5"
  ],
  "author": "likun",
  "license": "ISC",
  "sideEffects": false,
  "dependencies": {
    "@likun./lazy-img": "^1.0.18",
    "axios": "^0.26.1",
    "consola": "^2.15.3",
    "register-service-worker": "^1.7.2",
    "view-ui-plus": "^1.0.0-beta.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.9",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "babel-plugin-import": "^1.13.5",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "husky": "^7.0.4",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.71.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
